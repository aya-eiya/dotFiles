{
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "editor.tabSize": 2,
  "editor.fontFamily": "ゆたぽん,'Yutapon coding',Souce Han Sans JP, Menlo, Monaco, 'Courier New', monospace",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "eslint.validate": ["javascript", "javascriptreact", "vue"],
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.singleQuote": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
    "**/dist": true,
    "**/node_modules": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "prettier.trailingComma": "es5",
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "files.autoSave": "onFocusChange",
  "search.exclude": {
    "**/dist": true
  },
  "extensions.ignoreRecommendations": false,
  "dart.openDevTools": "flutter",
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false,
  "workbench.activityBar.visible": true,
  "editor.renderWhitespace": "all",
  "cSpell.userWords": [
    "Cupertino",
    "Intelli",
    "Millis",
    "NNBD",
    "RGBO",
    "Serializable",
    "TODOs",
    "allowlist",
    "androidx",
    "autolinkable",
    "ayabe",
    "eiya",
    "firestore",
    "graphiql",
    "graphql",
    "hoge",
    "interps",
    "ints",
    "macos",
    "multiline",
    "navi",
    "nohup",
    "promisify",
    "pubspecs",
    "repos",
    "respawn",
    "unawaited",
    "unfocus",
    "uuid",
    "vsync"
  ],
  "plantuml.render": "Local",
  "editor.renderControlCharacters": true,
  "dart_data_class_generator.fromJson.enabled": false,
  "dart_data_class_generator.fromMap.enabled": false,
  "dart_data_class_generator.hashCode.use_jenkins": true,
  "dart_data_class_generator.toJson.enabled": false,
  "dart_data_class_generator.toMap.enabled": false,
  "javascript.preferences.quoteStyle": "single",
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "git.autoRepositoryDetection": false,
  "git.countBadge": "off",
  "git.decorations.enabled": false,
  "git.detectSubmodules": false,
  "git.enableStatusBarSync": false,
  "git.openDiffOnClick": false,
  "git.showCommitInput": false,
  "git.showInlineOpenFileAction": false,
  "git.showProgress": false,
  "git.suggestSmartCommit": false,
  "git.smartCommitChanges": "tracked",
  "git.terminalAuthentication": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.blame.avatars": false,
  "gitlens.blame.dateFormat": "",
  "gitlens.currentLine.dateFormat": "",
  "gitlens.defaultDateFormat": "",
  "gitlens.defaultDateShortFormat": "",
  "gitlens.heatmap.ageThreshold": "",
  "gitlens.statusBar.dateFormat": "",
  "java.format.settings.url": "",
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "jest.pathToJest": "tests",
  "jest.rootPath": "src",
  "window.zoomLevel": -1,
  "terminal.integrated.scrollback": 9999,
  "java.requirements.JDK11Warning": false,
  "java.semanticHighlighting.enabled": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "debug.toolBarLocation": "docked",
  "cSpell.enableFiletypes": ["dart"],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "files.insertFinalNewline": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
  "settingsSync.ignoredExtensions": []
}
